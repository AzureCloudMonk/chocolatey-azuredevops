{
  "manifestVersion": 1,
  "id": "chocolatey-azuredevops",
  "version": "0.1.0",
  "name": "Chocolatey",
  "publisher": "gep13",
  "public": true,
  "description": "A set of Tasks related to using Chocolatey as part of your Azure DevOps Build Pipelines.",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "images/icon.png",
    "large": "images/icon-large.png"
  },
  "categories": [
    "Azure Pipelines"
  ],
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "links": {
    "learn": {
      "uri": "https://gep13.github.io/chocolatey-azuredevops/"
    },
    "license": {
      "uri": "https://github.com/gep13/chocolatey-azuredevops/blob/develop/LICENSE"
    },
    "repository": {
      "uri": "https://github.com/gep13/chocolatey-azuredevops"
    },
    "support": {
      "uri": "https://github.com/gep13/chocolatey-azuredevops/issues"
    }
  },
  "screenshots": [
    {
      "path": "images/task.png"
    }
  ],
  "content": {
    "details": {
      "path": "readme.md"
    }
  },
  "tags": [
    "chocolatey",
    "choco",
    "pack",
    "push",
    "package",
    "publish",
    "nuspec",
    "nupkg",
    "install",
    "upgrade"
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/gep13/chocolatey-azuredevops"
  },
  "contributions": [
    {
      "id": "chocolatey-azuredevops",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "Tasks/chocolatey"
      }
    }
  ],
  "files": [
    {
      "path": "Tasks/chocolatey"
    }
  ]
}
